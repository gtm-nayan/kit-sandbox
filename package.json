{
	"name": "kit-sandbox",
	"version": "0.0.1",
	"scripts": {
		"dev": "node ../kit/packages/kit/src/cli.js dev",
		"build": "node ../kit/packages/kit/src/cli.js build",
		"package": "node ../kit/packages/kit/src/cli.js package",
		"preview": "node ../kit/packages/kit/src/cli.js preview",
		"prepare": "node ../kit/packages/kit/src/cli.js sync",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-node": "next",
		"@sveltejs/adapter-static": "^1.0.0-next.29",
		"@sveltejs/kit": "next",
		"devalue": "^2.0.1",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"svelte": "^3.46.0",
		"typescript": "^4.6.2"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.0",
		"@lukeed/uuid": "^2.0.0",
		"cookie": "^0.4.1"
	},
	"pnpm": {
		"overrides": {
			"@sveltejs/kit": "../kit/packages/kit",
			"@sveltejs/adapter-auto": "../kit/packages/adapter-auto",
			"@sveltejs/adapter-node": "../kit/packages/adapter-node",
			"@sveltejs/adapter-static": "../kit/packages/adapter-static"
		}
	}
}
